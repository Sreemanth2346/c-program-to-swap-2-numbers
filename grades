#include <stdio.h>
int main(void){
int num;
printf("Enter your mark ");
scanf("%d",&num);
printf(" You entered %d", num);

	if(num >= 85){
	printf(" You got A grade");
		}
    else if (num>=70) {
    printf("Grade B");
    }
	else if ( num >=55){ 
		printf(" Grade C ");
		}
	else if ( num >=40){
		printf("  Grade D ");
		}
	else if ( num < 40){
		printf(" Grade F");
		}
return 0;
}
